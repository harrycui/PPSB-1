{
  "manifest_version": 2,
  "name": "PPSB",
  "description": "An extension for Privacy-Preserving Safe Browsing",
  "version": "1.6",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "storage",
    "unlimitedStorage"
  ],
  "options_page": "option.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "icon/icon_safe_32.png",
      "48": "icon/icon_safe_48.png",
      "128": "icon/icon_safe_128.png"
    },
    "default_title": "SafeBrowsing"
  },
  "web_accessible_resources": [ "block.html", "phishtank.json" ]
}
